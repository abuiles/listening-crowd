<div class="border">
  {{#ember-tether
     target='#side-content'
     targetAttachment='top left'
     attachment='top left'
  }}
    {{!-- <img class="p1" src={{model.itunesImage}} style="width: 200px;"> --}}
    <h4>{{model.title}}</h4>
    <ul>
      {{#each model.episodes as |episode|}}
        <li>
          {{link-to episode.title "podcasts.show.episode" model episode}}
        </li>
      {{/each}}
    </ul>
  {{/ember-tether}}
  <div class="sm-col sm-col-12 md-col-12  px2">
    <div class="border">
      {{liquid-outlet}}
    </div>
  </div>
</div>
